{
  "name": "browser-use-ts",
  "version": "0.1.0",
  "description": "TypeScript port of browser-use - a framework for building autonomous browser agents",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "dev": "ts-node src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "browser",
    "typescript",
    "browser-use",
    "playwright",
    "autonomous-agent",
    "llm",
    "ai",
    "web-automation"
  ],
  "author": "Pankil Joshi",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vadavision/browser-use-ts.git"
  },
  "bugs": {
    "url": "https://github.com/vadavision/browser-use-ts/issues"
  },
  "homepage": "https://github.com/vadavision/browser-use-ts#readme",
  "type": "commonjs",
  "dependencies": {
    "@langchain/openai": "^0.4.4",
    "@types/dotenv": "^6.1.1",
    "dotenv": "^16.4.7",
    "langchain": "^0.3.19",
    "node-fetch": "^2.6.7",
    "playwright": "^1.32.0",
    "turndown": "^7.2.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.11.30",
    "@types/node-fetch": "^2.6.3",
    "@types/turndown": "^5.0.5",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "eslint": "^8.37.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.7",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.2"
  }
}
